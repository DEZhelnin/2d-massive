#include  <iostream>
#include <ctime>
using namespace std;
int vvod(int** mas, int n, int m) {
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < m; j++) {
			mas[i][j] = rand() % 10 ;
		}
	}
	return 0;
}
int viy(int** mas, int n, int m) {
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < m; j++) {
			 cout << mas[i][j]<<" ";
		}
		cout << endl;
	}
	return 0;
}
int vsun(int** mas, int n, int m) {
	int iMin = 0;
	int jMin = 0;
	int min = mas[0][0];
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < m; j++) {
			if (min > mas[i][j]) {
				min = mas[i][j];
				iMin = i;
				jMin = j;
			}
		}
	}
		return (iMin);
}
int vsun1(int** mas, int n, int m) {
	int iMin = 0;
	int jMin = 0;
	int min = mas[0][0];
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < m; j++) {
			if (min > mas[i][j]) {
				min = mas[i][j];
				iMin = i;
				jMin = j;
			}
		}
	}
	return (jMin);
}
	int main() {
		setlocale(LC_ALL, "Rus");
		srand(time(0));
		int n;
		cin >> n ;
		int** mas = new int* [n];
		for (int i = 0; i < n; i++) {
			mas[i] = new int[n];
		}
		vvod(mas, n, n);
		viy(mas, n, n);
		int iMin=vsun(mas, n, n);
		int jMin = vsun1(mas, n, n);
		for (int i = 0; i < n; i++) {
				swap(mas[iMin][i], mas[i][jMin]);
			}
		viy(mas, n, n);
	}
